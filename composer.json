{
    "name": "smile/module-store-locator",
    "type": "magento2-module",
    "description": "Smile Store Locator",
    "keywords": ["magento2", "map", "geocoding", "geolocalisation", "retail"],
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "AurÃ©lien Foucret",
            "email": "aurelien.foucret@smile.fr"
        },
        {
            "name": "Romain Ruaud",
            "email": "romain.ruaud@smile.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "magento/framework": ">=103.0.4",
        "magento/module-store": ">=101.1.4",
        "magento/module-contact": ">=100.4.4",
        "smile/module-map": "^2.0",
        "smile/module-retailer": "^1.2",
        "willdurand/geocoder": "^4.4"
    },
    "require-dev": {
        "smile/magento2-smilelab-quality-suite": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Smile\\StoreLocator\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    }
}
